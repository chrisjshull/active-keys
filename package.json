{
  "name": "active-keys",
  "version": "0.0.1",
  "description": "Know and listen for which keys the user is currently holding down. Includes React helper.",
  "main": "dist/",
  "scripts": {
    "build": "npm run build:docs && npm run build:code",
    "build:docs": "documentation build src/** -f md -o docs.md",
    "build:code": "babel ./src --presets babel-preset-es2015,babel-preset-stage-2 --out-dir dist",
    "prepublishOnly": "npm run build",
    "preversion": "git diff-index --quiet HEAD --",
    "test": "eslint src"
  },
  "author": "chris@dremio.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "documentation": "^5.2.2",
    "eslint": "^4.5.0"
  },
  "dependencies": {
    "event-target-shim": "^2.0.0"
  }
}
